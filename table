package table;
import java.util.Scanner;

public class table {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn = new Scanner(System.in);
		System.out.print("Enter a number : ");
		int n = scn.nextInt();
		int value=0;
		for(int i=1;i<=10;i++) {
			value=n*i;
			System.out.println(n+"*"+i+"="+value);
		}
		System.out.println();
		

	}

}

import java.util.*;
public class NumberOfDigit {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int num = n;
        int count=0;
        while(num>0){
            num = num / 10;
            count++;
        }
        System.out.println("number of digits in "+n+" is "+count);

    }
    
}

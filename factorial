package factorials;

import java.util.Scanner;

public class fact {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.print("Enter a number : ");
		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		int fact=1;
		for(int i=2;i<=n;i++) {
			fact=fact*i;
		}
		System.out.println("factorial of "+n  +" is " +fact);
	}

}

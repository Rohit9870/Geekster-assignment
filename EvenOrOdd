import java.util.*;
public class oddoreven {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        if(n%2==0){
            System.out.println(n+" is a even number ");
        }
        else{
            System.out.println(n+" is a odd number ");
        }
    }
    
}

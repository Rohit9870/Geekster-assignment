package fibonacci;
import java.util.Scanner;


public class fibonacci {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn = new Scanner(System.in);
		int a =0;
		int b =1;
		int c;
		
		int n = scn.nextInt();
		System.out.print(a);
		System.out.print("\t"+b);
		for(int i=3;i<=n;i++) {
			c = a+b;
			a=b;
			b=c;
			System.out.print("\t"+c);
		}
	

	}

}
